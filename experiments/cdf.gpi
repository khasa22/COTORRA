reset;
set terminal pdfcairo font "Times-New-Roman,12:bold" size 17cm,8cm 
set datafile separator ","
set output "cdf-snr.pdf"
set xlabel 'SNR[dB]'
set ylabel 'CDF'

set ylabel font ",14"
set xlabel font ",14"
set grid front
set tics font ",14"
set key left top vertical Right noreverse enhanced autotitle
plot "snr.csv" using 2:(1.) smooth cnorm  title "our SNR", \
     "snr.csv" using 3:(1.) smooth cnorm  title "SNR [4]" , \
    "snr.csv" using 4:(1.) smooth cnorm  title "SNR [2]", \
